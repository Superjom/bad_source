  amznJQ.available("popover", function()
                   {
                     jQuery("#rewardTVDetailsLauncher").amazonPopoverTrigger
                       ({
                       showOnHover: false,
                       modal: false,
                       draggable: false,
                       showCloseButton: true,
                       forceAlignment: true,
                       skin: "default",
