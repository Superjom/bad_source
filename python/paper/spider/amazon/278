<script type="text/javascript">
  amznJQ.available("popover", function()
                   {
                     jQuery("#tvDetailsLauncher").amazonPopoverTrigger
                       ({
                       showOnHover: false,
                       modal: false,
                       draggable: false,
                       showCloseButton: true,
