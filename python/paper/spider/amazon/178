    (function($) {
      if($.fn.acrPopover) return;
      var popoverConfig = {
        showOnHover:true,
        showCloseButton: "ontouchstart" in document.documentElement,
        width: null,
        location:'bottom',
        locationAlign:'left',
        locationOffset:[-20,0],
        group: 'reviewsPopover',
