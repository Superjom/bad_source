<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 299], [385, 337], [425, 372], [466, 408], [522, 457]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41xj4McvkdL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41xj4McvkdL.jpg","main":{"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SX342_.jpg":[342,300],"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SX385_.jpg":[385,337],"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SX425_.jpg":[425,372],"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SX466_.jpg":[466,408],"http://ecx.images-amazon.com/images/I/81M29eUuQgL._SX522_.jpg":[522,457]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41-jw7nmfsL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41-jw7nmfsL.jpg","main":{"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SX342_.jpg":[342,384],"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SX385_.jpg":[385,432],"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SX425_.jpg":[425,477],"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SX466_.jpg":[466,523],"http://ecx.images-amazon.com/images/I/81sHIEJYjYL._SX522_.jpg":[522,586]}},{"hiRes":null,"thumb":"http://ecx.images-amazon.com/images/I/41qe1FVgk3L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41qe1FVgk3L.jpg","main":{"http://ecx.images-amazon.com/images/I/41qe1FVgk3L._SX342_.jpg":[342,107],"http://ecx.images-amazon.com/images/I/41qe1FVgk3L._SX385_.jpg":[385,120],"http://ecx.images-amazon.com/images/I/41qe1FVgk3L._SX425_.jpg":[425,133],"http://ecx.images-amazon.com/images/I/41qe1FVgk3L._SX466_.jpg":[466,145],"http://ecx.images-amazon.com/images/I/41qe1FVgk3L.jpg":[500,156]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61sHniU023L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21lyQKPDDbL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/i