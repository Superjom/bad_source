<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 342], [385, 385], [425, 425], [466, 466], [500, 500]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":null,"thumb":"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL.jpg","main":{"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL._SX342_.jpg":[342,342],"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL._SX385_.jpg":[385,385],"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL._SX425_.jpg":[425,425],"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL._SX466_.jpg":[466,466],"http://ecx.images-amazon.com/images/I/51n7Zv6LYKL.jpg":[500,500]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41sb3yft7eL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41sb3yft7eL.jpg","main":{"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SX342_.jpg":[342,342],"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SX385_.jpg":[385,385],"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SX425_.jpg":[425,425],"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SX466_.jpg":[466,466],"http://ecx.images-amazon.com/images/I/81ZLhHd3JzL._SX522_.jpg":[522,522]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41bqkt5uWNL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41bqkt5uWNL.jpg","main":{"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SY445_.jpg":[122,445],"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SY500_.jpg":[138,500],"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SY550_.jpg":[151,550],"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SY606_.jpg":[167,606],"http://ecx.images-amazon.com/images/I/71pbKzq3E7L._SY679_.jpg":[187,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/91xmqv6NwUL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41sKWdxNsJL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/i