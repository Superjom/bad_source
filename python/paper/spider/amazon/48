                'colorToAsin': {'initial': {}},
                'holderRatio': 0.77,
                'holderMaxHeight': 700
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.    
    return data;    
});
</script></div>


        
        
    
        
        
        
        




