<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 229], [385, 258], [425, 285], [466, 313], [522, 351]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51oTsK7Zq%2BL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51oTsK7Zq%2BL.jpg","main":{"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SX342_.jpg":[342,230],"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SX385_.jpg":[385,259],"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SX425_.jpg":[425,286],"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SX466_.jpg":[466,313],"http://ecx.images-amazon.com/images/I/61-NM0ZkZ4L._SX522_.jpg":[522,351]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21bbmtGWTeL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/21bbmtGWTeL.jpg","main":{"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SX342_.jpg":[342,227],"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SX385_.jpg":[385,256],"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SX425_.jpg":[425,282],"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SX466_.jpg":[466,309],"http://ecx.images-amazon.com/images/I/61uEelfR4QL._SX522_.jpg":[522,347]}},{"hiRes":"http://ecx.images-amazon.com/images/I/6150UZhesIL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21SnHZtuybL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/21SnHZtuybL.jpg","main":{"http://ecx.images-amazon.com/images/I/6150UZhesIL._SX342_.jpg":[342,227],"http://ecx.images-amazon.com/images/I/6150UZhesIL._SX385_.jpg":[385,256],"http://ecx.images-amazon.com/images/I/6150UZhesIL._SX425_.jpg":[425,282],"http://ecx.images-amazon.com/images/I/6150UZhesIL._SX466_.jpg":[466,309],"http://ecx.images-amazon.com/images/I/6150UZhesIL._SX522_.jpg":[522,347]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61-zoKAVD5L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/31uUP4SyE