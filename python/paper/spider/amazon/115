               var keyLookUp = selectedTechAttribute.attr('href');
               var title = selectedTechAttribute.html();
               selectedTechAttribute.amazonPopoverTrigger({
                   showOnHover:true, 
                   showCloseButton:true,  
                   attached:"true",
                   location:"right",                                
                   locationMargin:20,
                   locationAlign:"middle",
                   cacheable:"true",
                   title:title,
                   destination:keyLookUp,
                   ajaxErrorContent:'No help associated with this attribute'
               });                                   
           });                                            
       });
  });	

