     <script type="text/javascript">
          P.when('jQuery','ready').execute(function($) {
            jQuery("#abbWrapper input[id^=mbb-offeringID-]").click(function() {
              var flag = this.checked;
              jQuery("#abbWrapper input[id^=mbb-offeringID-]").each(function() {
                this.checked = false;
              });
              this.checked = flag;
            });
            jQuery("#buybox #quantity").change(function() {
              var quantity = jQuery(this).val();
              jQuery("#abbWrapper .abbListItem input[name^=quantity]").each(function() {
                this.value = quantity;
              });
            });
          });
