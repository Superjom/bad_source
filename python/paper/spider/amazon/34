<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 222], [385, 249], [425, 275], [466, 302], [522, 338]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51n7jN7YGxL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51n7jN7YGxL.jpg","main":{"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SX342_.jpg":[342,222],"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SX385_.jpg":[385,250],"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SX425_.jpg":[425,276],"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SX466_.jpg":[466,303],"http://ecx.images-amazon.com/images/I/91a7Sozc27L._SX522_.jpg":[522,339]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21Dyj00JNBL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/21Dyj00JNBL.jpg","main":{"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SX342_.jpg":[342,244],"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SX385_.jpg":[385,275],"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SX425_.jpg":[425,304],"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SX466_.jpg":[466,333],"http://ecx.images-amazon.com/images/I/61Gw%2BmO9b-L._SX522_.jpg":[522,373]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41O3zUTvtyL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41O3zUTvtyL.jpg","main":{"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SY445_.jpg":[135,445],"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SY500_.jpg":[152,500],"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SY550_.jpg":[167,550],"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SY606_.jpg":[184,606],"http://ecx.images-amazon.com/images/I/71M95aXVAJL._SY679_.jpg":[206,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71CQaFxzdyL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/3