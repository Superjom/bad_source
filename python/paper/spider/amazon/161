<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 299], [385, 337], [425, 372], [466, 408], [522, 457]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41onGaYUs5L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41onGaYUs5L.jpg","main":{"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SX342_.jpg":[342,300],"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SX385_.jpg":[385,337],"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SX425_.jpg":[425,372],"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SX466_.jpg":[466,408],"http://ecx.images-amazon.com/images/I/81Zg4--9kRL._SX522_.jpg":[522,457]}},{"hiRes":"http://ecx.images-amazon.com/images/I/817LTC-X80L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41a%2BX9VzbXL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41a%2BX9VzbXL.jpg","main":{"http://ecx.images-amazon.com/images/I/817LTC-X80L._SX342_.jpg":[342,304],"http://ecx.images-amazon.com/images/I/817LTC-X80L._SX385_.jpg":[385,342],"http://ecx.images-amazon.com/images/I/817LTC-X80L._SX425_.jpg":[425,378],"http://ecx.images-amazon.com/images/I/817LTC-X80L._SX466_.jpg":[466,414],"http://ecx.images-amazon.com/images/I/817LTC-X80L._SX522_.jpg":[522,464]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41wS8sA6M5L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41wS8sA6M5L.jpg","main":{"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SX342_.jpg":[342,304],"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SX385_.jpg":[385,342],"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SX425_.jpg":[425,378],"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SX466_.jpg":[466,414],"http://ecx.images-amazon.com/images/I/81HVL2MyJ3L._SX522_.jpg":[522,464]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61sHniU023L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/21lyQKPDDbL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/21lyQKPDDbL.jpg","main":{"http://ecx.images-amazon.com/images/I/61sHniU023L._SY445_.jpg":[221,445],"http://ecx.images-amazon.com/images/I/61sHniU023L._SY500_.jpg":[248,500],"http://ecx.images-amazon.com/images/I/61sHniU023L._SY550_.jpg":[273,550],"http://ecx.images-amazon.com/images/I/61sHniU023L._SY606_.jpg":[301,606],"http://ecx.images-amazon.com/images/I/61sHniU023L._SY679_.jpg":[337,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41k6Sys3IZL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41k6Sys3IZL.jpg","main":{"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SX342_.jpg":[342,309],"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SX385_.jpg":[385,347],"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SX425_.jpg":[425,383],"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SX466_.jpg":[466,420],"http://ecx.images-amazon.com/images/I/81CWB4EhNTL._SX522_.jpg":[522,471]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51hWZtO6W5L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51hWZtO6W5L.jpg","main":{"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SX342_.jpg":[342,107],"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SX385_.jpg":[385,120],"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SX425_.jpg":[425,133],"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SX466_.jpg":[466,145],"http://ecx.images-amazon.com/images/I/71sjkx7zjEL._SX522_.jpg":[522,163]}}]}, 
                'colorToAsin': {'initial': {}},
                'holderRatio': 0.77,
                'holderMaxHeight': 700
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.    
    return data;    
});
</script></div>


        
        
    
        
        
        
        




