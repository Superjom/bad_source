        destination: dest,
        group: "reviewsPopover",
        hoverHideDelay:300,
        location:"bottom",
        locationAlign:"left",
        locationOffset:[-20,-2],
        paddingLeft: 15,
        paddingBottom: 5,
        paddingRight: 15,
        showOnHover:true,
        showCloseButton: true,
        width: null,
        onShow: function(popover) { 
            jQuery(document).click(); 
        }
    })
});
});
