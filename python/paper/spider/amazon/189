<script type="text/javascript">
(function () {
    var useAUI = (typeof window.P === "object" && typeof window.P.when === "function");
        
    var redirectCallBack = function ($) {
        if (!$) { return; }
        
        var returnUrl = null;
        var redirectLink = "https://www.amazon.com/ap/signin?_encoding=UTF8&openid.assoc_handle=usflex&openid.claimed_id=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0%2Fidentifier_select&openid.identity=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0%2Fidentifier_select&openid.mode=checkid_setup&openid.ns=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0&openid.ns.pape=http%3A%2F%2Fspecs.openid.net%2Fextensions%2Fpape%2F1.0&openid.pape.max_auth_age=0&openid.return_to=https%3A%2F%2Fwww.amazon.com%2Fgp%2Fproduct%2FB00BF9MZY4%3Fie%3DUTF8%26keywords%3DTV%26qid%3D1383996774%26ref_%3Dcustrec_signin_inline_nav%26s%3Dtv%26sr%3D1-23";
        
        var custrecReturnURL = function($) {
            var newPathName = "https://" + window.location.host;
            var pathArray = window.location.pathname.split('/');
            var urlParameters = window.location.search.substring(1).split('&');
            if ($.browser.msie) {
                var wholeURL = $(location).attr('href');
                var tmpSplit = wholeURL.split('?');
                urlParameters = tmpSplit[tmpSplit.length - 1].split('&');
            }
            var urlPattern = /^ref=/;
            for ( i=0; i<pathArray.length; i++) {
                if (!urlPattern.test(pathArray[i]) && pathArray[i] != "") {
                    newPathName += "/" + pathArray[i];
                }
            }
            var paramPattern = /^ref_=/;
            newPathName += "?";
            for ( i=0; i<urlParameters.length; i++) {
                if (!paramPattern.test(urlParameters[i])) {
                    newPathName += urlParameters[i] + "&";
                }
            }
            newPathName = newPathName.replace(/undefined&$/,"");
            newPathName = newPathName.replace(/[&?]$/,"");
            return newPathName;
        }
        
        var custrecReplaceReturnURL = function(originalLink, signInType, refTag) {
            //signInType: 1 - inline ; 2 - lightbox ; 3 - redirect
        	var newLink = "";
        	var URLs = originalLink.split('?');
        	newLink = URLs[0] + "?";
        	var pathArray = URLs[1].split( '&' );
        	var paramPattern = null;
        	paramPattern = /^openid.return_to=/;
            for ( i=0; i<pathArray.length; i++) {
                if (!paramPattern.test(pathArray[i])) {
                    newLink += pathArray[i] + "&";
                } else {
                    var parameters = pathArray[i].split('=');
                    var tmpReturnURL = decodeURIComponent(parameters[1]);
                    var returnParameters = tmpReturnURL.split('?');
                    if (signInType == 1) {
                        paramPattern = /^custrec-return_=/;
                        var inlineReturnURL = returnParameters[0] + '?';
        	            var inlinePathArray = returnParameters[1].split( '&' );
        	            for ( v=0; v<pathArray.length; v++) {
        	                if (!paramPattern.test(inlinePathArray[v])) {
                                inlineReturnURL += inlinePathArray[v] + "&";
                            } else {
                                var inlineParameters = inlinePathArray[v].split('=');
                                inlineReturnURL += inlineParameters[0] + '=' + encodeURIComponent(returnUrl + '&ref_=' + refTag);
                            }
        	            }
        	            newLink += parameters[0] + '=' + encodeURIComponent(inlineReturnURL);
                    } else {
                        newLink += parameters[0] + '=' + encodeURIComponent(returnUrl + '&ref_=' + refTag);
                    }   
                }
            }
            newLink = newLink.replace(/[&?]$/,"");
            return newLink;
        }
        
        function signInPopUp(domArray, $) {
            domArray.bind('click',{$:$},function(event) {
                var $=event.data.$;
                if (0) {
                    returnUrl = custrecReturnURL($);
                    redirectLink = custrecReplaceReturnURL(redirectLink, 3, 'custrec_signin_inline_nav');
                }
                // use default sign in
                domArray.attr("href", redirectLink);
            });
        }
        var domArray= $(
            '#nav-your-account,' +
            '#sign-in-tooltip-body .cust-rec-aui-button.cust-rec-inline-tag .a-button-text,' +
            '#nav_your_account_flyout a.nav-action-button.nav-sprite'
        );
        
        signInPopUp(domArray, $);
    };
    if (useAUI) {
        //hook up AUI (trying to protect against a random global P that is not the AUI P)
        window.P.when('jQuery', 'ready', 'legacy-popover').execute(redirectCallBack);
    } else if (window.amznJQ) {
        //hook up amaznJQ
        window.amznJQ.available('jQuery', function() {
            window.amznJQ.available('popover', function() {
                window.amznJQ.jQuery(document).ready(function (){
                    redirectCallBack(window.amznJQ.jQuery);
                });
            });
        });
    }
})();
</script><a name="suggestion-box"> <br></a>
