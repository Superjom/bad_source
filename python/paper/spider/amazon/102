var twisterInitPrefetchMode = 0;

amznJQ.available("jQuery", function() {
amznJQ.onCompletion('amznJQ.criticalFeature', function() {
amznJQ.available("pageRefreshJS", function() {
  Twister["isLoggingEnabled"] = 0;
