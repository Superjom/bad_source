     </script><script type="text/javascript">
    var giftFlagScopeNumber = 0;
    function getGiftLogScope() {
        giftFlagScopeNumber++;
        return "ajax_giftFlag_log" + giftFlagScopeNumber;
    }

    function logGiftCheckboxMetrics(refTag) {
        var component = 'dp_giftFlag';
        var message = 'The gift flag refTag is  ' + refTag ;
        var logURL = '/gp/product/features/log-message.html/ref=' + refTag;
        var logScope = getGiftLogScope();
        if (window.ue) {
            uet("tc", logScope);
        }
        ajaxReq = jQuery.ajax({
            type: 'GET', 
            url: logURL,
            dataType: 'json',
            data: {component:component, message:message},
            success: function(data) {
                var logrequestId = data.rid;
                if (window.ue) {
                    ues("id", logScope, logrequestId);
                    ues("t0", logScope, new Date());
                    ues("ctb", logScope , "1");
                    uet("cf", logScope);
                    uet("be", logScope);
                    uex("ld", logScope);
                }
            }
        });
    }

    amznJQ.available('jQuery', function() {
        jQuery('#atc-gift-wrap-checkbox').click(function() {
            if (this.checked) {
                logGiftCheckboxMetrics('dp_giftFlag_checked');
            } else {
                logGiftCheckboxMetrics('dp_giftFlag_unchecked');
            }
        });
    });

</script><div id="isGift" class="a-section a-spacing-none a-text-left">
