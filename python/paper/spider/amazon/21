<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 342], [385, 385], [425, 425], [466, 466], [500, 500]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":null,"thumb":"http://ecx.images-amazon.com/images/I/51KxkzzeWCL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51KxkzzeWCL.jpg","main":{"http://ecx.images-amazon.com/images/I/51KxkzzeWCL._SX342_.jpg":[342,342],"http://ecx.images-amazon.com/images/I/51KxkzzeWCL._SX385_.jpg":[385,385],"http://ecx.images-amazon.com/images/I/51KxkzzeWCL._SX425_.jpg":[425,425],"http://ecx.images-amazon.com/images/I/51KxkzzeWCL._SX466_.jpg":[466,466],"http://ecx.images-amazon.com/images/I/51KxkzzeWCL.jpg":[500,500]}},{"hiRes":"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/31M2wgCxD8L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/31M2wgCxD8L.jpg","main":{"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SY445_.jpg":[281,445],"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SY500_.jpg":[316,500],"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SY550_.jpg":[348,550],"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SY606_.jpg":[383,606],"http://ecx.images-amazon.com/images/I/41YUI2vxpCL._SY679_.jpg":[429,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41i35rHBQ8L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/41i35rHBQ8L.jpg","main":{"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SY445_.jpg":[122,445],"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SY500_.jpg":[137,500],"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SY550_.jpg":[151,550],"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SY606_.jpg":[166,606],"http://ecx.images-amazon.com/images/I/610fcGUXu5L._SY679_.jpg":[187,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/41KYXsC2t7L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/311XWO-59yL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/i