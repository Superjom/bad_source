<script type="text/javascript">
maintainHeight = function(){
    var mainHolder = document.getElementById("main-image-container");
    var imgTagWrapperId = document.getElementById("imgTagWrapperId");
    if(mainHolder && typeof mainHolder != 'undefined'){
        var width = mainHolder.offsetWidth;
        var ratio = 0.77;
        var shouldAutoPlay = false;
        var videoSizes = [[342, 322], [385, 362], [425, 400], [466, 438], [500, 471]];
        
        var containerHeight = width/ratio;
        containerHeight = Math.min(containerHeight, 700);
        
        mainHolder.style.height = containerHeight + "px";
        if(imgTagWrapperId && typeof imgTagWrapperId !== 'undefined' ){
            imgTagWrapperId.style.height = containerHeight + "px";
        }
        
        if(shouldAutoPlay){
            var landingImage = document.getElementById("landingImage");
            
            if(landingImage){
                var autoPlayWidth=0, autoPlayHeight=0;
                for(var i=0; i < videoSizes.length; i++){
                    if( autoPlayWidth < videoSizes[i][0] ){
                        autoPlayWidth = videoSizes[i][0];
                        autoPlayHeight = videoSizes[i][1];
                    }
                }
                if( autoPlayWidth > 0 && autoPlayHeight > 0 ){
                    var videoRatio = autoPlayWidth/autoPlayHeight;
                    // increasing height of the slate image by 1px (and width with the ratio) so that black lines doesn't appear on ipad on right side.
                    var isIpad = navigator.userAgent.match(/ipad/i);
                    if( isIpad ){
                        containerHeight = containerHeight + 1;
                    }
                    landingImage.style.width = containerHeight * videoRatio + "px";
                    landingImage.style.height = containerHeight + "px";
                }
                
            }
        }
    }
};
maintainHeight();

window.onresize = function(){
    maintainHeight();
};
P.when('A').register("ImageBlockATF", function(A){   
    var data = {
                'colorImages': { 'initial': [{"hiRes":null,"thumb":"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL.jpg","main":{"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL._SX342_.jpg":[342,322],"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL._SX385_.jpg":[385,363],"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL._SX425_.jpg":[425,400],"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL._SX466_.jpg":[466,439],"http://ecx.images-amazon.com/images/I/51%2BNlsUvFlL.jpg":[500,471]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/31%2B4-bRgFeL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/31%2B4-bRgFeL.jpg","main":{"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SX342_.jpg":[342,228],"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SX385_.jpg":[385,257],"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SX425_.jpg":[425,283],"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SX466_.jpg":[466,311],"http://ecx.images-amazon.com/images/I/71jBO7XQ5oL._SX522_.jpg":[522,348]}},{"hiRes":"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/2138q39BVZL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/2138q39BVZL.jpg","main":{"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SY445_.jpg":[297,445],"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SY500_.jpg":[333,500],"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SY550_.jpg":[367,550],"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SY606_.jpg":[404,606],"http://ecx.images-amazon.com/images/I/61H44u5R6TL._SY679_.jpg":[453,679]}},{"hiRes":"http://ecx.images-amazon.com/images/I/71MxjQgGxmL._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/41T79Yrl9vL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.i